<h1>Edit cube <%=@cube.id%></h1>

<%= form_for @cube do |f| %>

<p>Size:<br/><%= f.text_field :size %> cm</p>
<p>Color:<br/><%= f.text_field :color %></p>
<p>Weight:<br/><%= f.text_field :weight %> kg</p>
<p>Material:<br/><%= f.text_field :material %></p>

<p><%= f.submit "Apply changes" %></p>

<% end %>